<p id="notice"><%= notice %></p>

<h1>Listing Lessonhours</h1>

<table>
  <thead>
    <tr>
      <th>Beginning</th>
      <th>Ending</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lessonhours.each do |lessonhour| %>
      <tr>
        <td><%= lessonhour.beginning %></td>
        <td><%= lessonhour.ending %></td>
        <td><%= link_to 'Show', lessonhour %></td>
        <td><%= link_to 'Edit', edit_lessonhour_path(lessonhour) %></td>
        <td><%= link_to 'Destroy', lessonhour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lessonhour', new_lessonhour_path %>
