<p id="notice"><%= notice %></p>

<h1>Listing Weeklyschedules</h1>

<table>
  <thead>
    <tr>
      <th>Lessonhours</th>
      <th>Day</th>
      <th>Curriculum</th>
      <th>Classroom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weeklyschedules.each do |weeklyschedule| %>
      <tr>
        <td><%= weeklyschedule.lessonhours %></td>
        <td><%= weeklyschedule.day %></td>
        <td><%= weeklyschedule.curriculum %></td>
        <td><%= weeklyschedule.classroom %></td>
        <td><%= link_to 'Show', weeklyschedule %></td>
        <td><%= link_to 'Edit', edit_weeklyschedule_path(weeklyschedule) %></td>
        <td><%= link_to 'Destroy', weeklyschedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weeklyschedule', new_weeklyschedule_path %>
